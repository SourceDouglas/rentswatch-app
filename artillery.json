{
  "config": {
    "target": "http://api.rentswatch.com",
    "phases": [
      {
        "duration": 600,
        "arrivalRate": 150
      }
    ],
    "payload": {
      "fields": ["lat", "lng"]
    },
    "variables": {
      "city": [ "vienna", "brussels", "zurich", "prague", "hamburg", "berlin", "copenhagen", "madrid", "barcelona", "paris", "rome", "amsterdam", "warsaw", "geneva"]
    }
  },
  "scenarios": [
    {
      "flow": [
        {
          "get": {
            "url": "/api/cities/geocode?q={{ lat }},{{ lng }}"
          }
        }
      ]
    },
    {
      "flow": [
        {
          "get": {
            "url": "/api/cities/{{ city }}"
          }
        }
      ]
    }
  ]
}
